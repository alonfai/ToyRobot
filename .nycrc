{
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.js"
    ],
    "watermarks": {
      "lines": [80, 95],
      "functions": [80, 95],
      "branches": [80, 95],
      "statements": [80, 95]
    },
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "cache": true,
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./coverage"
  }
